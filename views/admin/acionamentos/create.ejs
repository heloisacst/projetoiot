<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>Cadastro de Acionamentos</title>
  <%- include('../../import'); -%>
</head>

<body>
  <%- include('../../navs'); -%>
  <h1>Novo Dispositivo</h1>
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <form action="create" method="POST">
          <div class="form-group">
            <label for="acionamentoCode">Nome do Dispositivo</label>
            <input name="name" type="text" class="form-control" id="acionamentoName" aria-describedby="emailHelp"
              placeholder="Informe o nome do dispositivo">
          </div>
            <input name="status" type="hidden" value="0">
          <button type="submit" class="btn btn-primary">Salvar</button>
        </form>
      </div>
    </div>
  </div>
</body>

</html>