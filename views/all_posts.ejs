<!DOCTYPE html> 
<html>
  <head>
    <%- include('import'); -%>
  </head>
  <body>
     <%- include('navs'); -%>
      <div class="posts-detail">
        <h1><%= posts.name %></h1>
        <p><%= posts.description %></p>
      </div>
      <div class="container">
        <div class="row">
            <% posts.forEach((post) => { %>
            <%- include('post_card', { post: post }); -%>
            <% }); %>
        </div>
      </div>
      <%- include('footer'); -%>
  </body>
</html>